{
	"name": "jscs",
	"displayName": "JSCS Linting (deprecated)",
	"version": "0.0.1",
	"private": false,
	"description": "A JSCS Linter for Visual Studio Code",
	"keywords": [
		"land"
	],
	"homepage": "HTTPS://GitHub.Com/CodeEditorLand/LandJSCS#readme",
	"bugs": {
		"url": "HTTPS://GitHub.Com/CodeEditorLand/LandJSCS/issues"
	},
	"repository": {
		"type": "git",
		"url": "git+HTTPS://github.com/CodeEditorLand/LandJSCS.git"
	},
	"license": "SEE LICENSE IN LICENSE",
	"author": {
		"name": "üåÜ‚ÄÅ‚Äî‚ÄÅLand‚ÄÅ‚Äî",
		"email": "Land@PlayForm.Cloud",
		"url": "HTTPS://Land.PlayForm.Cloud"
	},
	"type": "module",
	"main": "./out/extension",
	"scripts": {
		"Document": "Document 'Source/**/*.ts'",
		"compile": "tsc -p ./",
		"prepublishOnly": "Build 'Source/**/*.ts'",
		"update-vscode": "node ./node_modules/vscode/bin/install"
	},
	"contributes": {
		"configuration": {
			"properties": {
				"jscs.configuration": {
					"default": {},
					"description": "A JSCS configuration object",
					"type": "object"
				},
				"jscs.disableIfNoConfig": {
					"default": false,
					"description": "Disable JSCS if no configuration file found.",
					"type": "boolean"
				},
				"jscs.enable": {
					"default": true,
					"description": "Control whether JSCS is enabled for JavaScript files or not.",
					"type": "boolean"
				},
				"jscs.preset": {
					"default": "",
					"description": "The JSCS preset to use. Possible values: \"airbnb\", \"crockford\", \"google\", \"grunt\", \"idiomatic\", \"jquery\", \"mdcs\", \"node-style-guide\", \"wikimedia\", \"wordpress\", \"yandex\"",
					"type": "string"
				}
			},
			"title": "JSCS configuration options",
			"type": "object"
		},
		"jsonValidation": [
			{
				"fileMatch": ".jscsrc",
				"url": "http://json.schemastore.org/jscsrc"
			}
		]
	},
	"activationEvents": [
		"onLanguage:javascript"
	],
	"dependencies": {},
	"devDependencies": {
		"@playform/build": "0.0.7",
		"@playform/document": "0.0.6"
	},
	"publishConfig": {
		"access": "public"
	},
	"icon": "JSCS_icon.png",
	"galleryBanner": {
		"color": "#37699A",
		"theme": "dark"
	}
}
